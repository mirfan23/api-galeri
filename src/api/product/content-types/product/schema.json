{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "mappedBy": "product"
    },
    "pelukis": {
      "type": "string",
      "required": true
    },
    "colors": {
      "type": "string",
      "default": "-"
    },
    "categories": {
      "type": "string",
      "default": "-"
    }
  }
}
