{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "pelukis": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true,
      "default": "no description"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "ukuran": {
      "type": "string",
      "default": "tidak diketahui",
      "required": true
    },
    "Tema": {
      "type": "string",
      "default": "-"
    }
  }
}
